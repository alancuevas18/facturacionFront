e/dist/rules\";\n\nextend(\"email\", email);\nextend(\"min\", min);\n\nextend(\"required\", required);\n\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      subscribe: true\n    };\n  },\n  methods: {\n    submit() {\n      alert(\"Form has been submitted!\");\n    }\n  }\n};\n</script>\n<style>\n.navbar-nav .nav-item p {\n  line-height: inherit;\n  margin-left: 5px;\n}\n</style>\n"]}]}