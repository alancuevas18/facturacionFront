\"checkbox\"\n        :disabled=\"disabled\"\n        v-model=\"model\"\n      />\n      <span class=\"form-check-sign\"></span>\n      <slot> <span v-if=\"inline\">&nbsp;</span> </slot>\n    </label>\n  </div>\n</template>\n<script>\nexport default {\n  name: 'base-checkbox',\n  model: {\n    prop: 'checked'\n  },\n  props: {\n    checked: [Array, Boolean],\n    disabled: Boolean,\n    inline: Boolean,\n    hasError: Boolean\n  },\n  data() {\n    return {\n      cbId: '',\n      touched: false\n    };\n  },\n  computed: {\n    model: {\n      get() {\n        return this.checked;\n      },\n      set(check) {\n        if (!this.touched) {\n          this.touched = true;\n        }\n        this.$emit('input', check);\n      }\n    },\n    inlineClass() {\n      if (this.inline) {\n        return `form-check-inline`;\n      }\n\n      return false;\n    }\n  },\n  created() {\n    this.cbId = Math.random()\n      .toString(16)\n      .slice(2);\n  }\n};\n</script>\n"]}]}