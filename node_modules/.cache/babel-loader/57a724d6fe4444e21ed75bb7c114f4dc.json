ator: val => {\n        return val.length > 1;\n      }\n    },\n    gradientStops: {\n      type: Array,\n      default: () => [1, 0.4, 0],\n      validator: val => {\n        return val.length > 1;\n      }\n    }\n  },\n  data() {\n    return {\n      ctx: null\n    };\n  },\n  methods: {\n    updateGradients(chartData) {\n      if (!chartData) return;\n      const ctx =\n        this.ctx || document.getElementById(this.chartId).getContext('2d');\n      const gradientStroke = ctx.createLinearGradient(0, 230, 0, 50);\n\n      this.gradientStops.forEach((stop, index) => {\n        gradientStroke.addColorStop(stop, this.gradientColors[index]);\n      });\n\n      chartData.datasets.forEach(set => {\n        if (!set.backgroundColor) {\n          set.backgroundColor = gradientStroke;\n        }\n      });\n    }\n  },\n  mounted() {\n    this.$watch(\n      'chartData',\n      (newVal, oldVal) => {\n        this.updateGradients(newVal);\n        if (!oldVal) {\n          this.renderChart(this.chartData, this.extraOptions);\n        }\n      },\n      { immediate: true }\n    );\n  }\n};\n"],"mappings":";;AAAA,SAASA,GAAG,EAAEC,MAAM,QAAQ,aAAa;AAEzC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,OAAO,EAAEH,GAAG;EACZC,MAAM,EAAE,CAACA,MAAM,CAACG,YAAY,CAAC;EAC7BC,KAAK,EAAE;IACLC,YAAY,EAAEC,MAAM;IACpBC,cAAc,EAAE;MACdC,IAAI,EAAEC,KAAK;MACXC,OAAO,EAAE;QAAA,OAAM,CACb,qBAAqB,EACrB,qBAAqB,EACrB,oBAAoB,CACrB;MAAA;MACDC,SAAS,EAAE,mBAAAC,GAAG,EAAI;QAChB,OAAOA,GAAG,CAACC,MAAM,GAAG,CAAC;MACvB;IACF,CAAC;IACDC,aAAa,EAAE;MACbN,IAAI,EAAEC,KAAK;MACXC,OAAO,EAAE;QAAA,OAAM,CAAC,CAAC,EAAE,GAAG,EAAE,CAAC,CAAC;MAAA;MAC1BC,SAAS,EAAE,mBAAAC,GAAG,EAAI;QAChB,OAAOA,GAAG,CAACC,MAAM,GAAG,CAAC;MACvB;IACF;EACF,CAAC;EACDE,IAAI,kBAAG;IACL,OAAO;MACLC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAe,2BAACC,SAAS,EAAE;MAAA;MACzB,IAAI,CAACA,SAAS,EAAE;MAChB,IAAMH,GAAG,GACP,IAAI,CAACA,GAAG,IAAII,QAAQ,CAACC,cAAc,CAAC,IAAI,CAACC,OAAO,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;MACpE,IAAMC,cAAc,GAAGR,GAAG,CAACS,oBAAoB,CAAC,CAAC,EAAE,