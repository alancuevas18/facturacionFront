 .onUpdate(function() {\n          vm.animatedNumber = this.tweeningNumber.toFixed(0);\n        })\n        .start();\n\n      animate();\n    }\n  },\n  mounted() {\n    this.initAnimation(this.value, 0);\n  },\n  watch: {\n    number(newValue, oldValue) {\n      this.initAnimation(newValue, oldValue);\n    }\n  }\n};\n</script>\n<style></style>\n"]}]}